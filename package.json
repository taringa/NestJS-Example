{
  "name": "nestjs-example",
  "version": "0.1.0",
  "description": "NestJS Example with scaffold to start coding using NestJS",
  "main": "dist/main.js",
  "scripts": {
    "coverage": "echo '0'",
    "update-docker-version": "sed -i.bck \"s|version=\\\"[0-9]*\\.[0-9]*\\.[0-9]*.*\\\"|version=\\\"${npm_package_version}\\\"|\" Dockerfile",
    "update-changelog": "node ./scripts/update-changelog.js ${npm_package_version}",
    "version": "npm run update-changelog && npm run update-docker-version && rm -fr Dockerfile.bck && git add CHANGELOG.md Dockerfile",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "git-cz",
    "docs": "typedoc --options typedoc.json",
    "prepare": "husky install"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@google-cloud/storage": "^7.0.1",
    "@mia-platform-internal/taringa-auth-library": "^0.14.0",
    "@mia-platform/custom-plugin-lib": "^6.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.3",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.1.3",
    "@nestjs/swagger": "7.1.7",
    "@nestjs/testing": "10.1.3",
    "axios": "^1.4.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "fastify": "^4.21.0",
    "husky": "8.0.3",
    "lodash": "4.17.21",
    "migrate-mongo": "^10.0.0",
    "module-alias": "2.2.3",
    "mongodb": "^5.7.0",
    "mongoose": "^7.4.2",
    "nestjs-cls": "^3.5.0",
    "nestjs-pino": "^3.3.0",
    "open": "9.1.0",
    "pino-http": "^8.4.0",
    "pino-pretty": "^10.2.0",
    "probe-image-size": "^7.2.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@mia-platform/eslint-config-mia": "^3.0.0",
    "@nestjs/cli": "10.1.11",
    "@nestjs/schematics": "10.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/lodash": "4.14.196",
    "@types/node": "20.4.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.46.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.0",
    "jest": "29.6.2",
    "prettier": "^3.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=14"
  },
  "private": true,
  "eslintConfig": {
    "extends": "@mia-platform/eslint-config-mia"
  },
  "jest": {
    "moduleNameMapper": {
      "^@shared(.*)$": "<rootDir>/src/components/shared$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@filters(.*)$": "<rootDir>/src/filters$1",
      "^@guards(.*)$": "<rootDir>/src/guards$1",
      "^@interfaces(.*)$": "<rootDir>/src/interfaces$1",
      "^@pipes(.*)$": "<rootDir>/src/pipes$1",
      "^@dto(.*)$": "<rootDir>/src/dto$1",
      "^@responses(.*)$": "<rootDir>/src/responses$1",
      "^@interceptors(.*)$": "<rootDir>/src/interceptors$1",
      "^@decorators(.*)$": "<rootDir>/src/decorators$1",
      "^@exceptions(.*)$": "<rootDir>/src/exceptions/v1$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/constants/",
      "<rootDir>/src/decorators/",
      "<rootDir>/src/exceptions/",
      "<rootDir>/src/filters/",
      "<rootDir>/src/guards/",
      "<rootDir>/src/interceptors/",
      "<rootDir>/src/interfaces/",
      "<rootDir>/src/pipes/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/modules/**/*.ts",
      "!**/*.{dto,interface,schema,module,strategy,constants}.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@shared": "./dist/components/shared",
    "@components": "./dist/components",
    "@filters": "./dist/filters",
    "@guards": "./dist/guards",
    "@interfaces": "./dist/interfaces",
    "@pipes": "./dist/pipes",
    "@dto": "./dist/dto",
    "@responses": "./dist/responses",
    "@interceptors": "./dist/interceptors",
    "@decorators": "./dist/decorators",
    "@utils": "./dist/utils"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
